{
  "version": 2,
  "buildCommand": "bash build-frontend-only.sh",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {}
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}